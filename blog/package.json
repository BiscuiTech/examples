{
  "name": "example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "node node_modules/.build/start",
    "clean": "rm -rf dist",
    "style": "prettier --write 'src/**/*'",
    "build": "yarn -s clean && pumpkins build",
    "dev": "pumpkins dev",
    "data:seed": "ts-node prisma/fake-data.ts",
    "postinstall": "yarn -s link pumpkins && chmod +x node_modules/.bin/pumpkins"
  },
  "dependencies": {
    "pumpkins": "../../pumpkins"
  },
  "devDependencies": {
    "@types/faker": "^4.1.8",
    "faker": "^4.1.0",
    "prettier": "^1.18.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
