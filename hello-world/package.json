{
  "name": "hello-world",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/": "zeit/ncc",
    "@types/joi": "^14.3.4",
    "@types/rollup-plugin-visualizer": "^2.6.0",
    "@zeit/ncc": "^0.22.1",
    "joi": "^14.3.1",
    "nexus": "^0.22.0-next.14",
    "nexus-plugin-prisma": "^0.8.0",
    "rollup": "^2.8.2",
    "rollup-plugin-visualizer": "^4.0.4",
    "ts-node": "^8.10.1"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "jest": "25.4.0",
    "jest-environment-node": "25.4.0",
    "prettier": "2.0.4",
    "ts-jest": "25.4.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node node_modules/.build",
    "build": "nexus build",
    "generate": "nexus generate",
    "dev": "nexus dev",
    "test": "jest",
    "doctor": "nexus doctor",
    "bundle": "ts-node --dir scripts rollup && du -h dist-bundle",
    "bundle:ncc": "ncc build node_modules/.build/index.js --stats-out dist-ncc/stats.json --out dist-ncc/bundle && du -h dist-ncc/bundle",
    "bundle:ncc2": "node scripts/ncc && du -h dist-ncc/bundle.js",
    "link:nexus": "yarn -s link nexus && chmod +x node_modules/.bin/nexus"
  },
  "prettier": {
    "semi": false
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "./tests/environment"
  }
}
